<odoo>
    <data>
         <record id="sequence_product_qr_name" model="ir.sequence">
            <field name="name">Product QR Code</field>
            <field name="code">product.qr.code</field>
            <field name="prefix">QR/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_product_qr_code" model="ir.sequence">
            <field name="name">Product QR Code Sequence</field>
            <field name="code">product.qr.code.sequence</field>
            <field name="prefix">QR</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>